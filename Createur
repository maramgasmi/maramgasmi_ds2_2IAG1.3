public class Createur extends Compte {
    private String specialite;

    public Createur(String nom,String prenom, String email, String mdp, String specialite) {
        super(nom, prenom, email, mdp);
        this.specialite = specialite;
    }

    @Override
    public void afficherDetails() {
        System.out.println("Créateur : " + nom + ", Spécialité : " + specialite);
    }

    public String getSpecialite() {
        return specialite;
    }
}
